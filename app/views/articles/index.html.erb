<h1>Articles listing page</h1>
<table>
    <th>
        <tr>
          <th>Title</th>
          <th>Description</th>
          <th>Actions</th>
        </tr>
    </th>

    <tbody>
        <% @allarticles.each do |article| %>
            <tr>
                <td><%= article.title %></td>
                <td><%= article.description %></td>
                <td>Place Holder</td>
            </tr>
        <% end %>
    </tbody>
</table>